"*":
  welcome:
    showOnStartup: false
  core:
    followSymlinks: true
    projectHome: "~/Documents/github"
    audioBeep: false
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".DS_STORE"
      ".idea"
    ]
  "atom-ctags":
    autoBuildTagsWhenActive: true
  linter:
    lintOnEditorFocus: false
    showInfoMessages: true
    statusBar: "Show error of the selected line"
    lintOnChange: false
    clearOnChange: true
    showErrorPanel: false
  editor:
    invisibles: {}
    preferredLineLength: 120
    showIndentGuide: true
  "tool-bar":
    position: "Left"
    visible: false
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
  tabs:
    enableVcsColoring: true
    tabScrolling: true
    usePreviewTabs: true
  "tree-view":
    hideIgnoredNames: true
  "symbols-tree-view":
    autoToggle: true
minimap:
  plugins:
    "git-diff": false
"autocomplete-plus":
  enableAutoActivation: false
editor:
  showIndentGuide: true
  softWrap: true
